<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Button Content="注册" Click="RegisterButton_OnClick" x:Name="RegisterButton" Height="26" Width="100"></Button>
            <Button Content="注销" Click="CancelRegisterButton_OnClick" x:Name="CancelRegisterButton" Height="26" Width="100"></Button>
        </StackPanel>
    </Grid>
</Window>
